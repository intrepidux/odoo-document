<odoo>
    <template id="assets" inherit_id="website.assets_editor" name="Editor assets">
        <xpath expr="//script[@src='/website/static/src/js/menu/new_content.js']" position="replace">
            <script type="text/javascript" src="/document_wiki/static/src/js/new_content.js" />
        </xpath>
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/document_wiki/static/src/js/document_wiki.js" />
            <script type="text/javascript" src="/document_wiki/static/src/js/page_properties.js" />
        </xpath>
    </template>
</odoo>
