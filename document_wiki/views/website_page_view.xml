<odoo>
    <data>
        <record id="website_page_form" model="ir.ui.view">
            <field name="name">website.page.form</field>
            <field name="model">website.page</field>
            <field name="inherit_id" ref="website.website_pages_form_view"/>
            <field name="arch" type="xml">
                <field name="menu_ids" position="after">
                    <label for="wiki_ids"/>
                    <field name="wiki_ids">
                        <tree editable="bottom">
                            <field name="page_id"/>
                        </tree>
                    </field>
                </field>
            </field>
        </record>
    </data>
</odoo>
